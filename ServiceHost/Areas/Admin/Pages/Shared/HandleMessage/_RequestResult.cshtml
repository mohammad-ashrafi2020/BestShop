@{
    var successData = TempData["Success"];
    var notFoundData = TempData["NotFound"];
    var errorData = TempData["Error"];
}
@if (successData != null)
{
    <input type="hidden" id="requestResult" value="@successData" />
    <script src="/js/handlerMessages.js"></script>
}
else if (errorData != null)
{
    <input type="hidden" id="requestResult" value="@errorData" />
    <script src="/js/handlerMessages.js"></script>
}
else if (notFoundData != null)
{
    <input type="hidden" id="requestResult" value="@notFoundData" />
    <script src="/js/handlerMessages.js"></script>
}
